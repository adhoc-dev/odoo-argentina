<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="action_aeroo_report_ar_stock_picking" model="ir.actions.report">
    <field name="name">Comprobante de Entrega</field>
    <field name="model">stock.picking</field>
    <field name="report_name">aeroo_report_ar_picking</field>
    <field name="report_type">aeroo</field>
    <field name="in_format">oo-odt</field>
    <field name="report_file">personalizations_tg2/report/aeroo_report_ar_picking.odt</field>
    <field name="tml_source">file</field>
    <field name="process_sep" eval="True"/>
    <field name="binding_model_id" ref="stock.model_stock_picking"/>
    <field name="binding_type">report</field>
    <field name="out_format" ref="report_aeroo.report_mimetypes_pdf_odt"/>
    <field name="print_report_name">'Comprobante de Entrega ' + (','.join(object.voucher_ids.mapped('number')))</field>
  </record>
  <record id="action_aeroo_report_ar_stock_remito" model="ir.actions.report">
    <field name="name">Remito pre-impreso</field>
    <field name="model">stock.picking</field>
    <field name="report_name">aeroo_report_ar_remito</field>
    <field name="report_type">aeroo</field>
    <field name="in_format">oo-odt</field>
    <field name="tml_source">file</field>
    <field name="process_sep" eval="True"/>
    <field name="binding_model_id" ref="stock.model_stock_picking"/>
    <field name="binding_type">report</field>
    <field name="out_format" ref="report_aeroo.report_mimetypes_pdf_odt"/>
    <field name="print_report_name">'Remito ' + (','.join(object.voucher_ids.mapped('number')))</field>
    <field name="report_file">personalizations_tg2/report/aeroo_report_ar_remito.odt</field>
  </record>
</odoo>
