<odoo>
  <!-- Form -->
  <record id="stock_request_order_form" model="ir.ui.view">
    <field name="name">stock.request.order.form</field>
    <field name="model">stock.request.order</field>
    <field name="inherit_id" ref="stock_request.stock_request_order_form" />
    <field name="arch" type="xml">
      <field name="warehouse_id" position="after">
        <field name="user_tag_allow_ids" invisible="1"/>
      </field>
      <field name="product_id" position="attributes">
        <attribute name="domain">[('tag_ids', 'in', parent.user_tag_allow_ids)]</attribute>
      </field>
    </field>
  </record>

</odoo>