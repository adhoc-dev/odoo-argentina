<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record model="ir.ui.view" id="personalizations_energesa.odoo_studio_helpdesk_e9aeb9b0-3dc5-40de-a464-bcbf413e59d4">
    <field name="name">Odoo Studio: helpdesk.ticket.form customization</field>
    <field name="model">helpdesk.ticket</field>
    <field ref="helpdesk.helpdesk_ticket_view_form" name="inherit_id"/>
    <field type="xml" name="arch">
      <data>
        <xpath expr="//field[@name='company_id']" position="replace">
          <field name="x_datetime_start" string="Fecha Inicio" attrs="{&quot;required&quot;: [&quot;&amp;&quot;,[&quot;team_id&quot;,&quot;=&quot;,3],[&quot;stage_id&quot;,&quot;!=&quot;,23]], &quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,3]]}"/>
          <field name="x_last_stage_update" attrs="{}" invisible="1"/>
        </xpath>
        <xpath expr="//field[@name='team_id']" position="attributes">
          <attribute name="attrs">{"invisible": [["team_id","!=",3]]}</attribute>
        </xpath>
        <xpath expr="//form[1]/sheet[1]/group[1]/group[1]/field[@name='user_id']" position="attributes">
          <attribute name="attrs">{}</attribute>
        </xpath>
        <xpath expr="//field[@name='partner_id']" position="attributes">
          <attribute name="attrs">{"required": [["team_id","=",3]]}</attribute>
          <attribute name="options">{'no_create_edit':True}</attribute>
        </xpath>
        <xpath expr="//field[@name='partner_name']" position="attributes">
          <attribute name="attrs">{"invisible": [["partner_id","!=",False]]}</attribute>
          <attribute name="readonly">1</attribute>
        </xpath>
        <xpath expr="//field[@name='partner_email']" position="attributes">
          <attribute name="attrs">{"invisible": [["partner_id","!=",False]]}</attribute>
          <attribute name="readonly">1</attribute>
        </xpath>
        <xpath expr="//form[1]/sheet[1]/group[1]/group[2]/field[@name='project_id']" position="attributes">
          <attribute name="attrs">{"invisible": [["use_helpdesk_timesheet","=",False]]}</attribute>
          <attribute name="options">{'no_create_edit':True}</attribute>
        </xpath>
        <xpath expr="//form[1]/sheet[1]/group[1]/group[2]/field[@name='task_id']" position="attributes">
          <attribute name="attrs">{"invisible": [["use_helpdesk_timesheet","=",False]]}</attribute>
        </xpath>
        <xpath expr="//field[@name='ticket_type_id']" position="attributes">
          <attribute name="attrs">{}</attribute>
          <attribute name="required">1</attribute>
          <attribute name="widget">many2one</attribute>
        </xpath>
        <xpath expr="//field[@name='tag_ids']" position="after">
          <field name="x_sale_order" domain="[[&quot;partner_id&quot;,&quot;=&quot;,partner_id]]" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,4]]}"/>
          <field name="x_dropbox_adress" placeholder="DirecciÃ³n Dropbox" string="Info" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;=&quot;,3]]}"/>
        </xpath>
        <xpath expr="//form[1]/sheet[1]/notebook[1]" position="inside">
          <page name="studio_page_gsXIw" string="Datos Extra">
            <group name="studio_group_gsXIw">
              <group name="studio_group_gsXIw_left">
                <field name="x_task_instaladores"/>
                <field name="x_task_partner_mobile" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,3]]}"/>
                <field name="x_task_parnter_phone" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,3]]}"/>
                <field name="x_task_partner_street" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,3]]}"/>
                <field name="x_task_partner_street2" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,3]]}"/>
                <field name="x_task_partner_city" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,3]]}"/>
                <field name="x_task_partner_state" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,3]]}"/>
                <field name="x_saleorder_mobile" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,4]]}"/>
                <field name="x_saleorder_phone" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,4]]}"/>
                <field name="x_sale_order_street" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,4]]}"/>
                <field name="x_saleorder_street2" attrs="{&quot;required&quot;: [], &quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,4]]}"/>
                <field name="x_saleorder_state" attrs="{&quot;invisible&quot;: [[&quot;team_id&quot;,&quot;!=&quot;,4]]}"/>
              </group>
              <group name="studio_group_gsXIw_right"/>
            </group>
          </page>
        </xpath>
      </data>
    </field>
  </record>
  <record model="ir.ui.view" id="personalizations_energesa.default_calendar_vie_6008a0cd-047b-4225-accb-b4fda8f163fe">
    <field name="name">Default calendar view for helpdesk.ticket</field>
    <field name="model">helpdesk.ticket</field>
    <field type="xml" name="arch">
      <calendar date_start="x_date_start" date_stop="" string="Default calendar view for helpdesk.ticket"/>
    </field>
  </record>
  <record model="ir.ui.view" id="personalizations_energesa.odoo_studio_helpdesk_570acb25-8f03-4457-abcf-a77b57acf429">
    <field name="name">Odoo Studio: helpdesk.ticket.kanban customization</field>
    <field name="model">helpdesk.ticket</field>
    <field ref="helpdesk.helpdesk_ticket_view_kanban" name="inherit_id"/>
    <field type="xml" name="arch">
      <data>
        <xpath expr="//field[@name='partner_id']" position="after">
          <field display="full" name="x_datetime_start"/>
        </xpath>
        <xpath expr="//kanban[1]/templates[1]/t[1]/div[1]/div[2]/div[5]/field[@name='priority']" position="after">
          <field display="full" name="ticket_type_id"/>
        </xpath>
      </data>
    </field>
  </record>
  <record model="ir.ui.view" id="personalizations_energesa.odoo_studio_default__4634630b-9407-4533-bc3c-33682985ed24">
    <field name="name">Odoo Studio: Default calendar view for helpdesk.ticket customization</field>
    <field name="model">helpdesk.ticket</field>
    <field ref="personalizations_energesa.default_calendar_vie_6008a0cd-047b-4225-accb-b4fda8f163fe" name="inherit_id"/>
    <field type="xml" name="arch">
      <data>
        <xpath expr="//calendar[1]" position="attributes">
          <attribute name="date_start">x_datetime_start</attribute>
          <attribute name="quick_add">false</attribute>
        </xpath>
      </data>
    </field>
  </record>
  <record model="ir.ui.view" id="personalizations_energesa.odoo_studio_helpdesk_7f1bd22a-275c-4be4-b94c-78012c96cbc9">
    <field name="name">Odoo Studio: helpdesk.ticket.tree customization</field>
    <field name="model">helpdesk.ticket</field>
    <field ref="helpdesk.helpdesk_tickets_view_tree" name="inherit_id"/>
    <field type="xml" name="arch">
      <data>
        <xpath expr="//field[@name='stage_id']" position="after">
          <field name="create_uid"/>
          <field name="create_date"/>
          <field name="x_last_stage_update"/>
        </xpath>
      </data>
    </field>
  </record>
</odoo>
