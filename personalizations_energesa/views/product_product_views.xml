<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record model="ir.ui.view" id="personalizations_energesa.odoo_studio_product__b728b227-d052-4a60-a3a3-f8d7dffdda36">
    <field name="name">Odoo Studio: product.product.tree customization</field>
    <field name="model">product.product</field>
    <field ref="account.product_product_view_tree" name="inherit_id"/>
    <field type="xml" name="arch">
      <data>
        <xpath expr="//field[@name='supplier_taxes_id']" position="replace">
          <field name="replenishment_cost_type"/>
          <field name="standard_price_copy"/>
        </xpath>
        <xpath expr="//field[@name='taxes_id']" position="replace">
          <field name="standard_price"/>
          <field name="cost_method"/>
          <field name="property_cost_method"/>
          <field name="computed_list_price"/>
          <field name="other_currency_list_price"/>
          <field name="list_price_type"/>
          <field name="computed_list_price_manual"/>
          <field name="cost_currency_id"/>
          <field name="replenishment_base_cost"/>
          <field name="replenishment_base_cost_on_currency"/>
          <field name="replenishment_cost_copy"/>
          <field name="replenishment_cost"/>
          <field name="total_cost"/>
        </xpath>
        <xpath expr="//field[@name='attribute_value_ids']" position="replace">
          <field name="categ_id"/>
        </xpath>
      </data>
    </field>
  </record>
</odoo>
