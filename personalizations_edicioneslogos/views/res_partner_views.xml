<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record model="ir.ui.view" id="personalizations_edicioneslogos.ir_ui_view_3306">
    <field name="name">res.partner.form.view</field>
    <field name="model">res.partner</field>
    <field name="inherit_id" ref="base.view_partner_form"/>
    <field name="arch" type="xml">
      <field name="user_id" position="after">
        <field name="team_id" completion="1"/>
      </field>
      <xpath expr="//group[@name='sale']//field[@name='user_id']" position="after">
          <field name="x_colegio"/>
      </xpath>
       <xpath expr="//page[@name='sales_purchases']" position="after">
        <page string="Proyectos">
          <field name="x_project_ids" string="Proyectos">
            <tree editable="top">
              <field name="x_campaign_id"/>
              <field name="x_project_ids" widget="many2many_tags"/>
            </tree>
          </field>
        </page>
      </xpath>
    </field>
  </record>
</odoo>
