<?xml version="1.0"?>
<odoo>
    <record id="view_warehouse_orderpoint_tree_editable_config" model="ir.ui.view">
        <field name="name">stock.warehouse.orderpoint.tree</field>
        <field name="model">stock.warehouse.orderpoint</field>
        <field name="inherit_id" ref="stock_ux.view_warehouse_orderpoint_tree_editable_config"/>
        <field name="arch" type="xml">
            <field name="warehouse_rotation_stdev" position="after">
                <field name="minimum_factor" optional="show"/>
                <field name="maximum_factor" optional="show"/>
                <field name="product_qty_min_suggested" optional="show"/>
                <field name="product_qty_max_suggested" optional="show"/>
            </field>
        </field>
    </record>
</odoo>
