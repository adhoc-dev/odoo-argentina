<?xml version="1.0"?>
<odoo>
    <record id="view_update_from_maxmin_from_suggested_wizard_form" model="ir.ui.view">
        <field name="name">Actualizar máximo y mínimo desde cantidades sugeridas</field>
        <field name="model">orderpoint.update_maxmin_from_suggested.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Product Catalog Report">
                Actualizar cantidades maximas y minimas de reglas seleccionadas desde cantidades sugeridas.
                <footer>
                    <button name="confirm" string="Confirm" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" special="cancel" class="oe_link"/>
                </footer>
            </form>
        </field>
    </record>

    <act_window id="action_update_maxmin_from_suggested_wizard"
        name="Actualizar máximo y mínimo desde cantidades sugeridas"
        res_model="orderpoint.update_maxmin_from_suggested.wizard"
        binding_model="stock.warehouse.orderpoint"
        view_mode="form" target="new" binding_views="list,form"/>

</odoo>
