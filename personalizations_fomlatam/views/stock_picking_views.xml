<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_picking_form_personalizations_fomlatam" model="ir.ui.view">
        <field name="name">view_picking_form_personalizations_fomlatam</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <field name="origin" position="after">
                <field name="date_etd" attrs="{'invisible':[('picking_type_code', '!=', 'incoming')]}"/>
                <field name="date_eta" attrs="{'invisible':[('picking_type_code', '!=', 'incoming')]}"/>
            </field>
        </field>
    </record>
    <record id="view_picking_tree_personalizations_fomlatam" model="ir.ui.view">
        <field name="name">view_picking_tree_personalizations_fomlatam</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="purchase_partner_ref" optional="show"/>
            </field>
            <field name="scheduled_date" position="before">
                <field name="date_etd" optional="show"/>
                <field name="date_eta" optional="show"/>
            </field>
        </field>
    </record>
</odoo>
