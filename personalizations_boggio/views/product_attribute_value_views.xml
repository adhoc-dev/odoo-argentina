<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_boggio.ir_ui_view_7911" model="ir.ui.view">
    <field name="name">Default search view for product.attribute.value</field>
    <field name="model">product.attribute.value</field>
    <field name="arch" type="xml">
      <search string="product.attribute.value">
        <field name="name" modifiers="{&quot;required&quot;: true}"/>
      </search>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_7912" model="ir.ui.view">
    <field name="name">Odoo Studio: Default search view for product.attribute.value customization</field>
    <field name="model">product.attribute.value</field>
    <field name="inherit_id" ref="personalizations_boggio.ir_ui_view_7911"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//search[1]/field[@name='name']" position="after">
          <field name="smart_search"/>
          <field name="attribute_id"/>
        </xpath>
      </data>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_7940" model="ir.ui.view">
    <field name="name">Odoo Studio: product.attribute.value.tree customization</field>
    <field name="model">product.attribute.value</field>
    <field name="inherit_id" ref="product.variants_tree_view"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//tree[1]/field[@name='name']" position="after">
          <field name="product_ids"/>
        </xpath>
      </data>
    </field>
  </record>
</odoo>
