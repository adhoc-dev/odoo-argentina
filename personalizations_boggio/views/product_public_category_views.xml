<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_boggio.ir_ui_view_4436" model="ir.ui.view">
    <field name="name">Boggio Personalization public.category.search</field>
    <field name="model">product.public.category</field>
    <field name="arch" type="xml">
      <search string="Search Category">
        <field name="name"/>
        <field name="parent_id"/>
      </search>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_8200" model="ir.ui.view">
    <field name="name">Odoo Studio: product.public.category.form customization</field>
    <field name="model">product.public.category</field>
    <field name="inherit_id" ref="website_sale.product_public_category_form_view"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='sequence']" position="after">
          <field name="x_google_product_category"/>
        </xpath>
      </data>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_8201" model="ir.ui.view">
    <field name="name">Odoo Studio: product.public.category.tree customization</field>
    <field name="model">product.public.category</field>
    <field name="inherit_id" ref="website_sale.product_public_category_tree_view"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='display_name']" position="after">
          <field name="x_google_product_category"/>
        </xpath>
      </data>
    </field>
  </record>
</odoo>
