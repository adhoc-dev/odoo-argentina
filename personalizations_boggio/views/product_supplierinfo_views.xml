<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_boggio.ir_ui_view_4601" model="ir.ui.view">
    <field name="name">ocultar precio de compra de producto iboggio</field>
    <field name="model">product.supplierinfo</field>
    <field name="inherit_id" ref="product.product_supplierinfo_tree_view"/>
    <field name="arch" type="xml">
        <field name="price" position="replace"/>
        <field name="min_qty" position="after">
          <field name="delay"/>
        </field>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_7797" model="ir.ui.view">
    <field name="name">Odoo Studio: product.supplierinfo.search.view customization</field>
    <field name="model">product.supplierinfo</field>
    <field name="inherit_id" ref="product.product_supplierinfo_search_view"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='product_tmpl_id']" position="attributes">
          <attribute name="string">Producto</attribute>
        </xpath>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_7807" model="ir.ui.view">
    <field name="name">Odoo Studio: product.supplierinfo.tree.view customization</field>
    <field name="model">product.supplierinfo</field>
    <field name="inherit_id" ref="product.product_supplierinfo_tree_view"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='product_id']" position="after">
          <field name="x_studio_field_5L26y" string="Codigo de proveedor"/>
          <field name="product_code"/>
        </xpath>
    </field>
  </record>
</odoo>
