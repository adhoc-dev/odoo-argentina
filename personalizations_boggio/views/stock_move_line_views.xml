<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_boggio.ir_ui_view_7654" model="ir.ui.view">
    <field name="name">agregar supplir code y internal code en process product</field>
    <field name="model">stock.move.line</field>
    <field name="inherit_id" ref="stock_ux.view_move_line_tree"/>
    <field name="arch" type="xml">
      <data>
        <field name="origin" position="after">
          <field name="x_supplier_code"/>
          <field name="x_internal_code"/>
        </field>
      </data>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_7660" model="ir.ui.view">
    <field name="name">Odoo Studio: stock.move.line.tree customization</field>
    <field name="model">stock.move.line</field>
    <field name="inherit_id" ref="stock.view_move_line_tree"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='reference']" position="after">
          <field name="origin"/>
        </xpath>
        <xpath expr="//field[@name='product_uom_id']" position="after">
          <field name="create_uid"/>
          <field name="picking_create_user_id"/>
        </xpath>
      </data>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_7880" model="ir.ui.view">
    <field name="name">Odoo Studio: stock.move.line.search customization</field>
    <field name="model">stock.move.line</field>
    <field name="inherit_id" ref="stock.stock_move_line_view_search"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//search[1]/field[@name='product_id']" position="replace"/>
        <xpath expr="//field[@name='picking_id']" position="before">
          <field name="product_id"/>
          <field name="x_supplier_code"/>
        </xpath>
        <xpath expr="//field[@name='product_id'][not(ancestor::field)]" position="before">
          <field name="product_barcode"/>
        </xpath>
      </data>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_8150" model="ir.ui.view">
    <field name="name">campos x_supplier_code	y x_product_brand_id</field>
    <field name="model">stock.move.line</field>
    <field name="inherit_id" ref="stock.view_stock_move_line_kanban"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='lot_id']//.." position="after">
        <div class="col-xs-6">
          <strong> Codigo:</strong>
          <field name="x_supplier_code"/>
          <strong> Marca: </strong>
          <field name="x_product_brand_id"/>
        </div>
      </xpath>
    </field>
  </record>
</odoo>
