<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_boggio.ir_ui_view_7925" model="ir.ui.view">
    <field name="name">Odoo Studio: product.attribute.tree customization</field>
    <field name="model">product.attribute</field>
    <field name="inherit_id" ref="product.attribute_tree_view"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='add_to_name']" position="after">
          <field name="x_chatbot_required"/>
          <field name="x_chatbot_name"/>
          <field name="id"/>
          <field name="x_chatbot_help"/>
          <field name="x_chatbot_question"/>
          <field name="x_chatbot_example"/>
        </xpath>
      </data>
    </field>
  </record>
</odoo>
