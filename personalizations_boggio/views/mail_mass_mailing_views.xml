<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_boggio.ir_ui_view_7980" model="ir.ui.view">
    <field name="name">Odoo Studio: mail.mass_mailing.tree customization</field>
    <field name="model">mail.mass_mailing</field>
    <field name="inherit_id" ref="mass_mailing.view_mail_mass_mailing_tree"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//tree[1]/field[@name='name']" position="after">
          <field name="sent_date"/>
        </xpath>
      </data>
    </field>
  </record>
</odoo>
