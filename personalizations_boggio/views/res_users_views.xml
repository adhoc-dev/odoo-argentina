<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_boggio.ir_ui_view_4620" model="ir.ui.view">
    <field name="name">email para preferencias de configuracion</field>
    <field name="model">res.users</field>
    <field name="inherit_id" ref="mail.view_users_form_mail"/>
    <field name="arch" type="xml">
      <field name="alias_contact" position="after">
        <field name="email"/>
        <field name="function"/>
        <field name="phone"/>
        <field name="mobile"/>
        <field name="main_id_number"/>
      </field>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_4936" model="ir.ui.view">
    <field name="name">datos personales en preferencia usuarios iboggio</field>
    <field name="model">res.users</field>
    <field name="inherit_id" ref="base.view_users_form_simple_modif"/>
    <field name="arch" type="xml">
      <field name="email" position="after">
        <field name="function"/>
        <field name="phone"/>
        <field name="mobile"/>
        <field name="main_id_number"/>
      </field>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_5160" model="ir.ui.view">
    <field name="name">agregar funcion en contactos iboggio</field>
    <field name="model">res.users</field>
    <field name="inherit_id" ref="base.view_users_simple_form"/>
    <field name="arch" type="xml">
      <field name="phone" position="before">
        <field name="function"/>
        <field name="email"/>
      </field>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_5543" model="ir.ui.view">
    <field name="name">Filtro usuarios portal</field>
    <field name="model">res.users</field>
    <field name="inherit_id" ref="base.view_users_search"/>
    <field name="arch" type="xml">
      <search>
        <filter name="Inactive" position="after"/>
        <filter name="portal" string="Portal" domain="[('share','=',True),('active','=','True')]"/>
      </search>
    </field>
  </record>
</odoo>
