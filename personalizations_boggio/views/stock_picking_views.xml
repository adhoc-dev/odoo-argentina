<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_boggio.ir_ui_view_4440" model="ir.ui.view">
    <field name="name">Boggio Personalization stock.picking.tree</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.vpicktree"/>
    <field name="arch" type="xml">
      <field name="date" position="replace"/>
      <field name="partner_id" position="after">
        <field name="date"/>
        <field name="date_done"/>
        <field name="x_so_create_uid"/>
        <field name="write_uid"/>
        <field name="picking_type_id"/>
      </field>
      <field name="origin" position="attributes">
        <attribute name="invisible">1</attribute>
      </field>
      <field name="state" position="after">
        <field name="x_user_id"/>
      </field>
      <field name="name" position="after">
        <field name="origin"/>
      </field>
      <tree position="attributes">
        <attribute name="default_order">date desc</attribute>
        <attribute name="multi_edit">0</attribute>
      </tree>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_4896" model="ir.ui.view">
    <field name="name">no permite cancelar en el despacho</field>
    <field name="model">stock.picking</field>
    <field eval="[(6, 0, [ref('personalizations_boggio.res_groups_162')])]" name="groups_id"/>
    <field name="inherit_id" ref="stock.view_picking_form"/>
    <field name="arch" type="xml">
      <button name="action_cancel" position="replace"/>
      <button name="do_unreserve" position="replace"/>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_4945" model="ir.ui.view">
    <field name="name">Boggio personalizations</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_form"/>
    <field name="arch" type="xml">
      <field name="product_type" position="after">
        <field name="x_supplier_code"/>
        <field name="x_product_brand_id"/>
        <button string="Traceability" name="%(personalizations_boggio.ir_act_server_1748)d" icon="fa-arrows-v" type="action"/>
        <button string="On Hand" name="%(personalizations_boggio.ir_act_server_1747)d" icon="fa-building-o" type="action"/>
        <button type="action" name="%(personalizations_boggio.ir_act_server_1746)d" class="oe_stat_button" icon="fa-search-plus" string="Resumen Movimiento"/>
      </field>
      <xpath expr="//div[@name='button_box']" position="inside">
        <button name="%(personalizations_boggio.ir_act_server_2338)d" class="oe_stat_button" icon="fa-truck" string="Albaranes Relacionados" type="action" attrs="{'invisible': [('group_id', '=', False)]}"/>
      </xpath>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_5611" model="ir.ui.view">
    <field name="name">stock.picking.internal.search.inherit</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_internal_search"/>
    <field name="arch" type="xml">
      <field name="name" position="after">
        <field name="x_user_id"/>
      </field>
      <filter name="picking_type" position="after">
        <filter name="x_user_id" string="Responsable" domain="[]" context="{'group_by':'x_user_id'}"/>
      </filter>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_5805" model="ir.ui.view">
    <field name="name">usuario2</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="personalizations_boggio.ir_ui_view_5611"/>
    <field name="arch" type="xml">
      <field name="x_user_id" position="replace"/>
      <field name="origin" position="after">
        <field name="x_user_id" domain="[('active', '=', True),'|', ('login', 'ilike', '@ingenieriaboggio.com.ar'), ('share', '=', False)]" readonly="True"/>
        <field name="x_write_uid2" domain="[('active', '=', True),'|', ('login', 'ilike', '@ingenieriaboggio.com.ar'), ('share', '=', False)]"/>
      </field>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_7740" model="ir.ui.view">
    <field name="name">Odoo Studio: stock.picking.form customization</field>
    <field name="model">stock.picking</field>
    <field name="priority" eval="199"/>
    <field name="inherit_id" ref="stock.view_picking_form"/>
    <field name="arch" type="xml">
      <field name="backorder_id" position="after">
        <field name="x_user_id" domain="[('share', '=', False)]"/>
        <field name="x_write_uid2"/>
        <field name="x_razon_devolucion"/>
      </field>
      <button name="do_print_picking" position="replace"/>
      <button name="action_toggle_is_locked" position="after">
        <button type="action" name="%(personalizations_boggio.ir_act_server_2449)d" string="Flete Propio" context="{ 'state_detail_ib': 2, 'ismodel':'stockpicking'}" barcode_trigger="F2" attrs="{'invisible': [ ('state', 'not in', ['done'])]}"/>
        <button type="action" name="%(personalizations_boggio.ir_act_server_2449)d" string="en Boggio" context="{ 'state_detail_ib': 1, 'ismodel':'stockpicking'}" barcode_trigger="F1" attrs="{'invisible': [ ('state', 'not in', ['done'])]}"/>
        <button type="action" name="%(personalizations_boggio.ir_act_server_2449)d" string="Recibido X Cliente" context="{ 'state_detail_ib': 3, 'ismodel':'stockpicking'}" barcode_trigger="F3" attrs="{'invisible': [ ('state', 'not in', ['done'])]}"/>
        <button type="action" name="%(personalizations_boggio.ir_act_server_2449)d" string="Flete Terceros" context="{ 'state_detail_ib': 10, 'ismodel':'stockpicking'}" barcode_trigger="F10" attrs="{'invisible': [ ('state', 'not in', ['done'])]}"/>
        <button type="action" name="%(personalizations_boggio.ir_act_server_2449)d" string="Martin" context="{ 'state_detail_ib': 11, 'ismodel':'stockpicking'}" barcode_trigger="F11" attrs="{'invisible': [ ('state', 'not in', ['done'])]}"/>
      </button>
      <!-- <button name="action_view_invoice" position="replace"> -->
      <!-- <field name="state" position="before">
        <button name="action_view_invoice" type="object" string="VER FACTURA" class="oe_highlight" attrs="{'invisible': [('invoice_ids', '=', [])]}" groups="base.group_user"/>
      </field> -->
      <field name="priority" position="after">
        <field name="printed" readonly="1"/>
        <field name="create_date"/>
        <field name="date_done"/>
      </field>
      <field name="book_id" position="attributes">
        <attribute name="widget">selection</attribute>
      </field>
      <!-- <xpath expr="//button[@name='button_scrap']" position="replace"/> -->
      <xpath expr="//field[@name='number_of_packages']" position="attributes">
        <attribute name="attrs">{}</attribute>
        <attribute name="force_save">True</attribute>
      </xpath>
      <xpath expr="//field[@name='declared_value']" position="after">
        <field name="x_plazo_pago"/>
        <field name="x_studio_field_T8yfU"/>
      </xpath>
      <xpath expr="//field[@name='printed']" position="after">
        <field name="x_rpa_link" widget="url"/>
      </xpath>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_5819" model="ir.ui.view">
    <field name="name">responsable picking (readonly=false)</field>
    <field name="model">stock.picking</field>
    <field eval="[(6, 0, [ref('personalizations_boggio.res_groups_190')])]" name="groups_id"/>
    <field name="inherit_id" ref="personalizations_boggio.ir_ui_view_7740"/>
    <field name="arch" type="xml">
      <field name="x_user_id" position="replace"/>
      <field name="x_write_uid2" position="before">
        <field name="x_user_id" readonly="False"/>
        <field name="date" readonly="True"/>
      </field>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_8136" model="ir.ui.view">
    <field name="name">Odoo Studio: stock.picking.internal.search customization</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_internal_search"/>
    <field name="arch" type="xml">
      <field name="product_id" position="after">
        <field name="x_product_brand_id"/>
      </field>
      <xpath expr="//field[@name='partner_id']" position="after">
        <field name="id"/>
      </xpath>
    </field>
  </record>
</odoo>
