          <button type="action" name="%(personalizations_boggio.ir_act_server_2452)d" string="en Boggio" context="{ 'state_detail_ib': 1, 'ismodel':'stockpicking'}" barcode_trigger="F1" attrs="{'invisible': [ ('state', 'not in', ['done'])]}"/>
          <button type="action" name="%(personalizations_boggio.ir_act_server_2453)d" string="Recibido X Cliente" context="{ 'state_detail_ib': 3, 'ismodel':'stockpicking'}" barcode_trigger="F3" attrs="{'invisible': [ ('state', 'not in', ['done'])]}"/>
          <button type="action" name="%(personalizations_boggio.ir_act_server_2454)d" string="Flete Terceros" context="{ 'state_detail_ib': 10, 'ismodel':'stockpicking'}" barcode_trigger="F10" attrs="{'invisible': [ ('state', 'not in', ['done'])]}"/>
          <button type="action" name="%(personalizations_boggio.ir_act_server_2455)d" string="Martin" context="{ 'state_detail_ib': 11, 'ismodel':'stockpicking'}" barcode_trigger="F11" attrs="{'invisible': [ ('state', 'not in', ['done'])]}"/>
        </button>
