  <record id="personalizations_boggio.ir_ui_view_7740" model="ir.ui.view">
    <field name="name">Odoo Studio: stock.picking.form customization</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_form"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//button[@name='button_scrap']" position="replace"/>
        <xpath expr="//field[@name='number_of_packages']" position="attributes">
          <attribute name="attrs">{}</attribute>
          <attribute name="force_save">True</attribute>
        </xpath>
        <xpath expr="//field[@name='declared_value']" position="after">
          <field name="x_plazo_pago"/>
          <field name="x_studio_field_T8yfU"/>
        </xpath>
        <xpath expr="//form[1]/sheet[1]/notebook[1]/page[1]/field[@name='move_line_ids']/tree[1]/field[@name='product_id']" position="after">
          <field name="x_product_brand_id"/>
          <field name="x_supplier_code"/>
        </xpath>
        <xpath expr="//form[1]/sheet[1]/notebook[1]/page[2]/field[@name='move_lines']/tree[1]" position="after">
          <form string="Stock Moves" create="0" edit="0">
            <header>
              <field name="state" widget="statusbar" statusbar_visible="draft,confirmed,assigned,done" modifiers="{&quot;readonly&quot;: true}"/>
            </header>
            <sheet name="studio_sheet_bef750">
              <div class="oe_button_box" name="button_box">
                <button name="action_get_account_moves" icon="fa-usd" class="oe_stat_button" string="Accounting Entries" type="object"/>
                <field name="purchase_id" invisible="1" can_create="true" can_write="true" modifiers="{&quot;readonly&quot;: true, &quot;invisible&quot;: true}"/>
                <button name="action_view_linked_record" class="oe_stat_button" icon="fa-credit-card" string="Purchase Order" type="object" context="{'res_id': purchase_id, 'action_ref': 'purchase.purchase_form_action', 'form_view_ref': 'purchase.purchase_order_form'}" attrs="{'invisible': [('purchase_id', '=', False)]}" modifiers="{&quot;invisible&quot;: [[&quot;purchase_id&quot;, &quot;=&quot;, false]]}">
                </button>
                <field name="sale_id" invisible="1" can_create="true" can_write="true" modifiers="{&quot;readonly&quot;: true, &quot;invisible&quot;: true}"/>
                <button name="action_view_linked_record" class="oe_stat_button" icon="fa-book" string="Sale Order" type="object" context="{'res_id': sale_id, 'action_ref': 'sale.action_orders', 'form_view_ref': 'sale.view_order_form'}" attrs="{'invisible': [('sale_id', '=', False)]}" modifiers="{&quot;invisible&quot;: [[&quot;sale_id&quot;, &quot;=&quot;, false]]}">
                </button>
                <field name="request_order_id" invisible="1" can_create="true" can_write="true" modifiers="{&quot;readonly&quot;: true, &quot;invisible&quot;: true}"/>
                <button name="action_view_linked_record" class="oe_stat_button" icon="fa-cubes" string="Request Orders" type="object" context="{'res_id': request_order_id, 'action_ref': 'stock_request.stock_request_order_action', 'form_view_ref': 'stock_request.stock_request_order_form'}" attrs="{'invisible': [('request_order_id', '=', False)]}" modifiers="{&quot;invisible&quot;: [[&quot;request_order_id&quot;, &quot;=&quot;, false]]}">
                </button>
              </div>
              <group name="studio_group_d5734d">
                <group name="main_grp" colspan="2">
                  <group name="main_grp_col1">
                    <field name="reference" modifiers="{&quot;readonly&quot;: true}"/>
                    <field name="location_id" can_create="true" can_write="true" modifiers="{&quot;required&quot;: true}"/>
                    <field name="location_dest_id" can_create="true" can_write="true" modifiers="{&quot;required&quot;: true}"/>
                  </group>
                  <group name="main_grp_col2">
                    <field name="product_id" on_change="1" can_create="true" can_write="true" modifiers="{&quot;readonly&quot;: [[&quot;state&quot;, &quot;in&quot;, [&quot;done&quot;]]], &quot;required&quot;: true}"/>
                    <label for="product_uom_qty"/>
                    <div class="o_row" name="studio_div_4b5a1f">
                      <field name="product_uom_qty" modifiers="{&quot;readonly&quot;: [[&quot;state&quot;, &quot;in&quot;, [&quot;done&quot;]]], &quot;required&quot;: true}"/>
                      <field name="product_uom" options="{'no_open': True, 'no_create': True}" on_change="1" can_create="true" can_write="true" studio_groups="[{&quot;id&quot;: 22, &quot;display_name&quot;: &quot;Technical Settings / Manage Multiple Units of Measure&quot;, &quot;name&quot;: &quot;Manage Multiple Units of Measure&quot;}]" modifiers="{&quot;required&quot;: true}"/>
                    </div>
                    <field name="name" invisible="1" on_change="1" modifiers="{&quot;invisible&quot;: true, &quot;required&quot;: true}"/>
                    <field name="invoice_line_ids" widget="many2many_tags" can_create="true" can_write="true" studio_groups="[{&quot;id&quot;: 26, &quot;display_name&quot;: &quot;Accounting &amp; Finance / Billing and Payments&quot;, &quot;name&quot;: &quot;Billing and Payments&quot;}]" modifiers="{&quot;readonly&quot;: true}"/>
                    <field name="date_expected" invisible="1" on_change="1" modifiers="{&quot;readonly&quot;: [[&quot;state&quot;, &quot;in&quot;, [&quot;done&quot;]]], &quot;invisible&quot;: true, &quot;required&quot;: true}"/>
                    <field name="date" attrs="{'invisible': [('state', '!=', 'done')]}" modifiers="{&quot;readonly&quot;: [[&quot;state&quot;, &quot;in&quot;, [&quot;done&quot;]]], &quot;invisible&quot;: [[&quot;state&quot;, &quot;!=&quot;, &quot;done&quot;]], &quot;required&quot;: true}"/>
                  </group>
                </group>
                <group name="origin_grp" string="Origin">
                  <field name="picking_id" readonly="1" on_change="1" can_create="true" can_write="true" modifiers="{&quot;readonly&quot;: true}"/>
                  <field name="origin" modifiers="{}"/>
                  <field name="group_id" on_change="1" can_create="true" can_write="true" modifiers="{}"/>
                  <field name="procure_method" attrs="{'readonly': [('state', '!=', 'draft')]}" studio_groups="[{&quot;id&quot;: 44, &quot;display_name&quot;: &quot;Technical Settings / Manage Push and Pull inventory flows&quot;, &quot;name&quot;: &quot;Manage Push and Pull inventory flows&quot;}]" modifiers="{&quot;readonly&quot;: [[&quot;state&quot;, &quot;!=&quot;, &quot;draft&quot;]], &quot;required&quot;: true}"/>
                  <field name="purchase_line_id" on_change="1" can_create="true" can_write="true" modifiers="{&quot;readonly&quot;: true}"/>
                </group>
                <group name="linked_group" string="Linked Moves" colspan="2" studio_groups="[{&quot;id&quot;: 8, &quot;display_name&quot;: &quot;Extra Rights / Technical Features&quot;, &quot;name&quot;: &quot;Technical Features&quot;}]" invisible="1" modifiers="{&quot;invisible&quot;: true}">
                  <field name="move_orig_ids" string="Origin Moves" readonly="1" can_create="true" can_write="true" modifiers="{&quot;readonly&quot;: true}">
                                </field>
                  <field name="move_dest_ids" string="Destination Moves" readonly="1" can_create="true" can_write="true" modifiers="{&quot;readonly&quot;: true}">
                                </field>
                </group>
                <newline/>
                <group name="allocations" string="Stock Request Allocations">
                  <field name="allocation_ids" modifiers="{}"/>
                </group>
              </group>
            </sheet>
          </form>
        </xpath>
        <xpath expr="//field[@name='printed']" position="after">
          <field name="x_rpa_link" widget="url"/>
        </xpath>
      </data>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_8131" model="ir.ui.view">
    <field name="name">orden de vista</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.vpicktree"/>
    <field name="arch" type="xml">
      <data>
        <!-- ordernar por secuencia de website -->
        <tree position="attributes">
          <attribute name="default_order">create_date desc</attribute>
        </tree>
      </data>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_8136" model="ir.ui.view">
    <field name="name">Odoo Studio: stock.picking.internal.search customization</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_internal_search"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='group_id']" position="after">
          <field name="id"/>
        </xpath>
      </data>
    </field>
  </record>
</odoo>
