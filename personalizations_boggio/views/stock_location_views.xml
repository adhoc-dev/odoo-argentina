<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_boggio.ir_ui_view_8016" model="ir.ui.view">
    <field name="name">Odoo Studio: stock.location.tree customization</field>
    <field name="model">stock.location</field>
    <field name="inherit_id" ref="stock.view_location_tree2"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='usage']" position="after">
        <field name="x_stk_warehouse_id"/>
        <field name="x_orden"/>
        <field name="x_nombre_viejo"/>
      </xpath>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_8017" model="ir.ui.view">
    <field name="name">Odoo Studio: stock.location.form customization</field>
    <field name="model">stock.location</field>
    <field name="inherit_id" ref="stock.view_location_form"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='partner_id']" position="after">
        <field name="x_stk_warehouse_id"/>
      </xpath>
      <xpath expr="//field[@name='barcode']" position="after">
        <field name="x_orden"/>
        <field name="x_nombre_viejo"/>
      </xpath>
    </field>
  </record>
</odoo>
