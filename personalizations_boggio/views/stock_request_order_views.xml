<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_boggio.ir_ui_view_7677" model="ir.ui.view">
    <field name="name">Odoo Studio: stock.request.order.tree customization</field>
    <field name="model">stock.request.order</field>
    <field name="inherit_id" ref="stock_request.stock_request_order_tree"/>
    <field name="arch" type="xml">
      <xpath expr="//tree[1]/field[@name='name']" position="before">
        <field name="create_date"/>
      </xpath>
      <xpath expr="//tree[1]/field[@name='name']" position="after">
        <field name="procurement_group_id"/>
        <field name="x_rotacion_local"/>
      </xpath>
      <xpath expr="//field[@name='location_id']" position="after">
        <field name="create_uid"/>
      </xpath>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_7735" model="ir.ui.view">
    <field name="name">Default search view for stock.request.order</field>
    <field name="model">stock.request.order</field>
    <field name="arch" type="xml">
      <search string="Stock Request Order">
        <field name="name" modifiers="{&quot;required&quot;: true, &quot;readonly&quot;: [[&quot;state&quot;, &quot;not in&quot;, [&quot;draft&quot;]]]}"/>
      </search>
    </field>
  </record>
  <record id="personalizations_boggio.ir_ui_view_7736" model="ir.ui.view">
    <field name="name">Odoo Studio: Default search view for stock.request.order customization</field>
    <field name="model">stock.request.order</field>
    <field name="inherit_id" ref="personalizations_boggio.ir_ui_view_7735"/>
    <field name="arch" type="xml">
      <xpath expr="//search[1]/field[@name='name']" position="after">
        <field name="procurement_group_id"/>
      </xpath>
    </field>
  </record>
</odoo>
