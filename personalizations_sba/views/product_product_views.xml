<odoo>
  <record id="personalizations_sba.odoo_studio_product__d2adfdd6-da7c-4acf-8e98-6bfa068606d3" model="ir.ui.view">
    <field name="name">Odoo Studio: Product Kanban customization</field>
    <field name="model">product.product</field>
    <field ref="product.product_kanban_view" name="inherit_id"/>
    <field name="arch" type="xml">
        <xpath expr="//kanban[1]/templates[1]/t[1]/div[1]/div[2]/strong[1]/field[@name='name']" position="after">
          <field display="false" name="x_studio_field_5G9jj"/>
        </xpath>
    </field>
  </record>
  <record id="personalizations_sba.odoo_studio_product__9ca64b94-1d20-4315-a820-ad8af930fc02" model="ir.ui.view">
    <field name="name">Odoo Studio: product.product.tree customization</field>
    <field name="model">product.product</field>
    <field ref="product.product_product_tree_view" name="inherit_id"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='default_code']" position="before">
          <field name="x_studio_field_5G9jj"/>
        </xpath>
        <xpath expr="//field[@name='virtual_available']" position="after">
          <field name="x_studio_field_NlwHP"/>
        </xpath>
    </field>
  </record>
  <record id="personalizations_sba.odoo_studio_product__0e53611d-abd5-4817-b1a8-3edc5f8fb3eb" model="ir.ui.view">
    <field name="name">Odoo Studio: product.stock.tree.2 customization</field>
    <field name="model">stock.valuation.layer</field>
    <field ref="stock_account.stock_valuation_layer_tree" name="inherit_id"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='product_id']" position="before">
          <field name="barcode"/>
        </xpath>
    </field>
  </record>
</odoo>
