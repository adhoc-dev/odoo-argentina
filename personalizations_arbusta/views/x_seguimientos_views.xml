    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='x_name']" position="before">
          <field name="x_studio_field_5UmOW"/>
        </xpath>
        <xpath expr="//field[@name='x_name']" position="after">
          <field name="x_studio_field_TTmDr"/>
          <field name="x_studio_field_nZW1U"/>
          <field name="x_studio_field_q3WdL"/>
        </xpath>
      </data>
    </field>
  </record>
