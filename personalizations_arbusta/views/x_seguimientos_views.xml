    <field name="arch" type="xml">
      <data>
        <xpath expr="//form[1]/sheet[1]" position="before">
          <header>
            <field name="x_studio_field_q3WdL" options="{'clickable': '1'}" widget="statusbar"/>
          </header>
        </xpath>
        <xpath expr="//group[@name='studio_group_7016ea_left']" position="inside">
          <field name="x_studio_field_5UmOW"/>
          <field name="x_studio_field_TTmDr"/>
          <field name="x_studio_field_2bqg4"/>
          <field name="x_studio_field_f0rK8"/>
        </xpath>
        <xpath expr="//group[@name='studio_group_7016ea_right']" position="inside">
          <field name="x_studio_field_nZW1U"/>
          <field name="x_studio_instancia_del_pal" string="Instancia del PAL" attrs="{'invisible': [['x_studio_field_nZW1U','!=','PAL']],'required': [['x_studio_field_nZW1U','=','PAL']],}"/>
          <field name="x_studio_instancia_del_proceso" string="Instancia del Proceso" attrs="{&quot;invisible&quot;: [[&quot;x_studio_field_nZW1U&quot;,&quot;!=&quot;,&quot;Casos puntuales&quot;]]}"/>
          <field name="x_studio_field_2Ct4K" attrs="{'invisible': ['&amp;',['x_studio_field_nZW1U','!=','Plan de Acción'],['x_studio_field_nZW1U','!=','PAL']],'required': ['|',['x_studio_field_nZW1U','=','Plan de Acción'],['x_studio_field_nZW1U','=','PAL']]}" widget="url"/>
          <field name="x_studio_field_HIWV1" attrs="{'invisible': ['&amp;',['x_studio_field_nZW1U','!=','Plan de Acción'],['x_studio_field_nZW1U','!=','PAL']],'required': ['|',['x_studio_field_nZW1U','=','Plan de Acción'],['x_studio_field_nZW1U','=','PAL']]}"/>
          <field name="x_studio_field_qDZi1" attrs="{'invisible': ['&amp;',['x_studio_field_nZW1U','!=','Plan de Acción'],['x_studio_field_nZW1U','!=','PAL']],'required': ['|',['x_studio_field_nZW1U','=','Plan de Acción'],['x_studio_field_nZW1U','=','PAL']]}"/>
        </xpath>
        <xpath expr="//group[@name='studio_group_7016ea']" position="after">
          <group name="studio_group_e0AEi">
            <group name="studio_group_e0AEi_left">
              <field name="x_studio_field_vzUZ2"/>
            </group>
            <group name="studio_group_e0AEi_right">
              <field name="x_studio_field_9EuDY" string="Confidencial" groups="personalizations_arbusta.empleados_confidenci_d42a72d7-91d7-45b3-a6fd-19d7edb94e91"/>
            </group>
          </group>
        </xpath>
        <xpath expr="//form[1]/sheet[1]" position="after">
          <div class="oe_chatter" name="studio_div_ef8755">
            <field name="message_follower_ids" widget="mail_followers"/>
            <field name="activity_ids" widget="mail_activity"/>
            <field name="message_ids" widget="mail_thread"/>
          </div>
        </xpath>
      </data>
    </field>
  </record>
  <record id="personalizations_arbusta.odoo_studio_default__350a0bee-97c9-4cb3-b220-f24677c62dec" model="ir.ui.view">
