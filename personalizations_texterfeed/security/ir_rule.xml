<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="personalizations_texterfeed.ir_rule_receips" model="ir.rule">
    <field name="name">restriccion_recibos_comerciales_permisos_minimos</field>
    <field ref="account_payment_group.model_account_payment_group" name="model_id"/>
    <field name="domain_force">[('x_comercial_id', '=', user.id)]</field>
    <field eval="[(4, ref('sales_team.group_sale_salesman'))]" name="groups"/>
  </record>
  <record id="personalizations_texterfeed.ir_rule_all_receips" model="ir.rule">
    <field name="name">todos los recibos</field>
    <field ref="account_payment_group.model_account_payment_group" name="model_id"/>
    <field name="domain_force">[(1, '=', 1)]</field>
    <field eval="[(4, ref('sales_team.group_sale_salesman_all_leads'))]" name="groups"/>
  </record>
</odoo>
